import numpy as np

# Quarterly sales data (example values â€“ replace with your actual data)
sales_data = np.array([120000, 150000, 170000, 210000])

# 1. Total sales for the year
total_sales = np.sum(sales_data)

# 2. Percentage increase from Q1 to Q4
q1_sales = sales_data[0]
q4_sales = sales_data[3]

percentage_increase = ((q4_sales - q1_sales) / q1_sales) * 100

# Output results
print("Quarterly Sales:", sales_data)
print("Total Sales for the Year:", total_sales)
print("Percentage Increase from Q1 to Q4: {:.2f}%".format(percentage_increase))
